<h1>
    <%= @organization.full_name %>
</h1>

<%= render :partial => "market", :object => @market %>

<%- unless @agents.empty? -%>
    <table class="resource sortable">
        <thead>
            <tr>
                <%- S4::Agent.schema.each do |element| -%>
                    <td class="<%= element.name %>">
                        <span><%= h element.title %></span>
                    </td>
                <%- end -%>
            </tr>
        </thead>
        <tbody>
            <%= render :partial => "shared/s4/resource_as_table_row", :collection => @agents %>
            <%- reset_cycle -%>
        </tbody>
    </table>
<%- end -%>
